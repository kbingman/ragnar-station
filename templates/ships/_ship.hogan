<a data-new="ship" href="/ships/new">New Ship</a>
{{# starships }}
  <div id="ship-{{ id }}" class="r-ship">
    <a href="/ships/{{ id }}" class="r-action r-ship-name" data-edit="{{ id }}">
      {{ name }}
    </a>
    {{# round }}{{ mass }}{{/ round }}m {{ configuration }}
    <button class="r-delete r-action" data-delete="{{ id }}">Remove</button>
  </div>
{{/ starships }}
